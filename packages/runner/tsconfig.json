{
  "extends": "../ts/tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react",
    "outDir": "dist",
    "noImplicitThis": false,
    "preserveWatchOutput": true,
    "strict": false,
    "types": [
      "node",
      "cypress",
      "cypress-real-events",
      "react",
    ],                           /* Type declaration files to be included in compilation. */
    "noErrorTruncation": true
  },
  "exclude": [
    "dist",
    "test"
  ]
}
