  "module": "es2020",
  "workspaces": {
    "nohoist": [
      "@toycode/markdown-it-class",
      "patch-package"
    ]
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "generate-shiki-theme"
        ],
        "outputs": [
          "{projectRoot}/src/generated/schema-for-tests.gen.json"
        ]
      }
    },
    "implicitDependencies": [
      "@packages/data-context",
      "@packages/graphql"
    ]
  }
